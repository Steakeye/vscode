{
  "name": "teamhub",
  "version": "14.0.2",
  "publisher": "TeamHub",
  "engines": {
    "vscode": "^1.54.3"
  },
  "icon": "resources/logo.png",
  "galleryBanner": {
    "color": "#013",
    "theme": "dark"
  },
  "displayName": "GitLive",
  "description": "Extend VS Code with real-time collaborative superpowers",
  "homepage": "https://git.live",
  "repository": {
    "type": "git",
    "url": "https://github.com/GitLiveApp/GitLive"
  },
  "bugs": {
    "url": "https://github.com/GitLiveApp/GitLive/issues"
  },
  "categories": [
    "SCM Providers"
  ],
  "keywords": [
    "real time",
    "live sharing",
    "remote share",
    "share code",
    "collaboration"
  ],
  "tags": [
    "real time",
    "live",
    "live share",
    "live sharing",
    "remote share",
    "remote sharing",
    "share code",
    "collaboration",
    "share",
    "sharing",
    "code discussion",
    "cursor navigation",
    "git",
    "git diff",
    "diff",
    "git blame",
    "pull request",
    "git history",
    "github",
    "bitbucket",
    "azure",
    "azure devops",
    "vscode must-have extensions",
    "vscode",
    "visual studio code"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gitlive",
          "title": "GitLive",
          "icon": "./resources/logo.svg"
        }
      ]
    },
    "views": {
      "gitlive": [
        {
          "id": "gitlive",
          "name": ""
        }
      ]
    },
    "configuration": {
      "title": "GitLive",
      "properties": {
        "GitLive.Offline Mode": {
          "type": "boolean",
          "scope": "application",
          "markdownDescription": "When enabled you will appear as offline"
        },
        "GitLive.Hide my activity graph": {
          "type": "boolean",
          "scope": "application",
          "markdownDescription": "When enabled your activity graph will not appear for you or your team"
        },
        "GitLive.Hide my working copy changes": {
          "type": "boolean",
          "scope": "application",
          "markdownDescription": "When enabled your local file changes will not appear for you or your team"
        },
        "GitLive.Hide others changes in gutter": {
          "type": "boolean",
          "scope": "application",
          "markdownDescription": "When enabled others changes will not show in the gutter for any file"
        },
        "GitLive.Issue tracker integration": {
          "type": "string",
          "scope": "application",
          "enum": [
            "Disabled",
            "Enabled",
            "Select issue tracker..."
          ]
        }
      }
    },
    "commands": [
      {
        "command": "git.live.toggleGutterIndicators",
        "title": "GitLive: Show other's changes",
        "enablement": "git.live.toggleGutterIndicators.enabled",
        "icon": {
          "light": "./resources/light/icon-logo.svg",
          "dark": "./resources/dark/icon-logo.svg"
        }
      },
      {
      "command": "git.live.hideGutterIndicators",
        "title": "GitLive: Hide other's changes",
        "enablement": "git.live.toggleGutterIndicators.enabled",
        "icon": {
          "light": "./resources/light/logo-light-bg.svg",
          "dark": "./resources/dark/logo-dark-bg.svg"
        }
      },
      {
        "command": "git.live.pushedChanges",
        "title": "✔ Pushed changes",
        "enablement": "git.live.pushedChanges.enabled"
      },
      {
        "command": "git.live.unpushedChanges",
        "title": "✔ Unpushed changes",
        "enablement": "git.live.unpushedChanges.enabled"
      },
      {
        "command": "git.live.pushedChangesOff",
        "title": "Pushed changes",
        "enablement": "git.live.pushedChanges.enabled"
      },
      {
        "command": "git.live.unpushedChangesOff",
        "title": "Unpushed changes",
        "enablement": "git.live.unpushedChanges.enabled"
      },
      {
        "command": "git.live.diff.pushedChanges",
        "title": "✔ Pushed changes",
        "enablement": "git.live.diff.pushedChanges.enabled"
      },
      {
        "command": "git.live.diff.unpushedChanges",
        "title": "✔ Unpushed changes",
        "enablement": "git.live.diff.unpushedChanges.enabled"
      },
      {
        "command": "git.live.diff.pushedChangesOff",
        "title": "Pushed changes",
        "enablement": "git.live.diff.pushedChanges.enabled"
      },
      {
        "command": "git.live.diff.unpushedChangesOff",
        "title": "Unpushed changes",
        "enablement": "git.live.diff.unpushedChanges.enabled"
      },
      {
        "command": "git.live.showAccountList",
        "title": "Manage your accounts",
        "icon": {
          "light": "./resources/light/icon-account.svg",
          "dark": "./resources/dark/icon-account.svg"
