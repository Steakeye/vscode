    "menus": {
      "editor/title": [
        {
          "command": "git.live.toggleGutterIndicators",
          "group": "navigation",
          "when": "git.live.activeEditorType != gitlive-diff && git.live.activeEditorType != gitlive && !git.live.toggleGutterIndicators.selected"
        },
        {
          "command": "git.live.hideGutterIndicators",
          "group": "navigation",
          "when": "git.live.activeEditorType != gitlive-diff && git.live.activeEditorType != gitlive && git.live.toggleGutterIndicators.selected"
        },
        {
          "submenu": "editor/title/viewOptions",
          "group": "navigation@90",
          "when": "git.live.activeEditorType == gitlive-diff"
        }
      ],
      "commandPalette": [
        {
          "command": "git.live.callPalette",
